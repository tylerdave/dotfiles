[user]
	name = Dave Forgac
	email = tylerdave@tylerdave.com
[core]
	autocrlf = input
	safecrlf = true

[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:'%h %ad | %s%d [%an <%ae>]' --graph --date=short
    last = diff HEAD^ HEAD
    type = cat-file -t
    dump = cat-file -p
[color]
	ui = true
[push]
	default = simple
